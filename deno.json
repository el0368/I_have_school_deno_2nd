{
  "nodeModulesDir": "manual",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update .",
    "precommit": "deno fmt && deno lint",
    "test": "deno test -A --unstable-kv tests/",
    "seed": "deno run -A --unstable-kv scripts/seed_db.ts",
    "wasm": "bash scripts/build_wasm.sh",
    "math": "cd math_engine && cargo run --release",
    "math:dev": "cd math_engine && cargo run"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@/": "./",
    "@testing-library/preact": "npm:@testing-library/preact@^3.2.4",
    "fresh": "jsr:@fresh/core@^2.2.0",
    "happy-dom": "npm:happy-dom@^20.7.0",
    "playwright": "npm:playwright@^1.58.2",
    "preact": "npm:preact@^10.27.2",
    "@preact/signals": "npm:@preact/signals@^2.5.0",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.8",
    "rehype-katex": "npm:rehype-katex@^7.0.1",
    "remark-math": "npm:remark-math@^6.0.0",
    "remark-gfm": "npm:remark-gfm@^4.0.0",
    "hast-util-from-parse5": "npm:hast-util-from-parse5@8.0.2",
    "vite": "npm:vite@^7.1.3",
    "@mdx-js/rollup": "npm:@mdx-js/rollup@^3.1.1",
    "@mdx-js/preact": "npm:@mdx-js/preact@^3.1.1"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
